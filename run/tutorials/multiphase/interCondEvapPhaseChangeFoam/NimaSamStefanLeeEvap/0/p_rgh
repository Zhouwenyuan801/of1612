FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e6; 

boundaryField
{
    rightWall
    {
        type            fixedFluxPressure;
    }
    
	leftWall
    {
        type            fixedFluxPressure;
    }

    topBottom
    {
        type            empty;
    }
    frontAndBack
    {
        type            empty;
    }
}


