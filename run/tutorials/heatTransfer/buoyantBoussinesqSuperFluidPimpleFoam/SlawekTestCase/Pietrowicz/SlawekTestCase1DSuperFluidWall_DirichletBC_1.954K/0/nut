FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    UP
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    DOWN
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    SYM
    {
        type            empty;
    }
    ADIABATIC
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
}


